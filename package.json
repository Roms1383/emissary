  "dependencies": {
    "@actions/core": "^1.9.1",
    "@octokit/core": "^4.0.5",
    "@octokit/graphql": "^5.0.1",
    "@octokit/rest": "^19.0.4",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@octokit/types": "^7.2.0",
    "@types/jest": "^28.1.7",
    "@vercel/ncc": "^0.34.0",
